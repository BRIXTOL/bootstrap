---
title: BRIXTOL | STRAP
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <title>
      BRIXTOL | STRAP
    </title>
    <link
      content="stylesheet"
      href="./bootstrap.css"
      rel="stylesheet">
    <style>

      .language-scss .token.property {
        color: #8be9fd;
      }

      .language-scss .token.punctuation {
        color: #ff79c6
      }

      .language-scss .token.string,
      .style .token.string,
      .token.entity,
      .token.operator,
      .token.url,
      .token.variable {
        color: #f8f8f2;
      }

      .language-scss .token.variable {
        color: #fff;
      }

      .namespace {
        opacity: .7;
      }

      .relapse {
        border: none;
        border-top: none;
        display: block;
        position: relative;
        width: 100%;
      }

      .relapse-btn {
        -moz-user-select: none;
        -ms-user-select: none;
        -webkit-user-select: none;
        background-color: inherit;
        border: none;
        border-radius: 0;
        border-top: 1px solid #e5e5e5;
        color: inherit;
        cursor: pointer;
        display: inherit;
        font-size: inherit;
        margin: 0;
        padding: 20px 0;
        text-align: left;
        user-select: none;
        width: 100%;
      }

      .relapse-btn.focused {
        background-color: inherit;
        outline: none;
      }

      .relapse-btn.initial {
        background-color: inherit;
        color: inherit;
      }

      .relapse-btn.initial + .relapse-fold {
        max-height: unset;
        opacity: 1;
        visibility: visible;
      }

      .relapse-btn.opened {
        background-color: inherit;
        color: inherit;
      }

      .relapse-fold {
        -webkit-transition: opacity 300ms linear, max-height 225ms ease-in-out;
        margin: 0 !important;
        max-height: 0;
        opacity: 0;
        overflow: hidden;
        padding: 0 !important;
        transition: opacity 300ms linear, max-height 225ms ease-in-out;
        will-change: opacity, max-height;
      }

      .relapse-fold >:first-child {
        padding: 50px;
      }

      .relapse-fold.expanded {
        max-height: auto;
        opacity: 1;
        visibility: visible;
      }

      .token.atrule,
      .token.attr-value,
      .token.class-name,
      .token.function {
        color: #f1fa8c;
      }

      .token.attr-name,
      .token.builtin,
      .token.char,
      .token.inserted,
      .token.selector,
      .token.string {
        color: #50fa7b;
      }

      .token.bold,
      .token.important {
        font-weight: bold;
      }

      .token.boolean,
      .token.number {
        color: #bd93f9;
      }

      .token.cdata,
      .token.comment,
      .token.doctype,
      .token.prolog {
        color: #999999;
      }

      .token.constant,
      .token.deleted,
      .token.property,
      .token.symbol,
      .token.tag {
        color: #ff79c6;
      }

      .token.entity {
        cursor: help;
      }

      .token.important,
      .token.regex {
        color: #ffb86c;
      }

      .token.italic {
        font-style: italic;
      }

      .token.keyword {
        color: #8be9fd;
      }

      .token.punctuation {
        color: #f8f8f2;
        }:not(pre) > code[class*='language-'] {
          border-radius: .3em;
          padding: .1em;
          white-space: normal;
        }

        @media(prefers-reduced-motion: reduce) {
          .relapse-fold {
            -webkit-transition: none;
            transition: none;
          }
        }

        code[class*='language-'],
        pre[class*='language-'] {
          -moz-hyphens: none;
          -moz-tab-size: 4;
          -ms-hyphens: none;
          -o-tab-size: 4;
          -webkit-hyphens: none;
          background: none;
          color: #f8f8f2;
          font-family: Consolas, Monaco, 'Andale Mono', 'Ubuntu Mono', monospace;
          hyphens: none;
          line-height: 1.5;
          tab-size: 4;
          text-align: left;
          text-shadow: 0 1px rgba(0, 0, 0, .3);
          white-space: pre;
          word-break: normal;
          word-spacing: normal;
          word-wrap: normal;
        }

        /* Code blocks */
        pre[class*='language-'] {
          border-radius: .3em;
          margin: .5em 0 2em;
          overflow: auto;
          padding: 1.5em 1.2em;
        }

        pre[class*='language-'] {
          background: #282a36;
        }; /* Inline code */
    </style>
    <script defer src="./bundle.global.js"></script>

  </head>
  <body>
    <div class="container">
      <div class="row g-0 jc-center">

        <div class="col-2 bd-right">
          <div class="py-5" id="sidebar">
            {% for item in classes %}
              <a class="d-block mb-3 upcase" href="{{ item | first | slugify | prepend: '#' }}">
                {{ item | first |replace: "_", " " }}
              </a>
            {% endfor %}
          </div>
        </div>

        <div class="col-10 bg-white pl-5 my-5">

          {% for item in classes %}

            {% assign slug = item | first | slugify %}
            {% assign name = item | first %}
            {% assign prop = classes[name] %}


            <h3 class="fw-bold d-flex ai-center jc-between pt-3 pb-2" id="{{ slug }}">
              {{ prop.title }}
              <code class="lower">
                {{ prop.title | handle }}
              </code>
            </h3>

            {% assign demo = item | first | slugify | append: "/demo.liquid" %}
            {% render demo, item: prop %}


            {% assign demo = item | first | slugify | append: "/html.liquid" %}
            {% render demo, item: prop %}


            {% assign demo = item | first | slugify | append: "/scss.liquid" %}
            {% render demo, item: prop %}

            <hr>
          {% endfor %}


        </div>
      </div>
    </div>
  </body>
</html>
