{% for item in item.classes %}
  <div class="row ai-center mb-2">
    <div class="col-2">
      <pre class="m-1"><code>{{ item.class }}</code></pre>
    </div>
    <div class="col my-0">
      <span class="{{ item.class | remove_first: '.' }}">
        {{ item.title }}
      </span>
    </div>
  </div>
{% endfor %}
