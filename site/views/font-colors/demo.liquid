{% for item in item.classes %}
  <div class="row ai-center mb-2">
    <div class="col-2">
      <pre class="m-1"><code>{{ item.class }}</code></pre>
    </div>
    <div class="col my-0">
      {%- if item.class == ".fc-white" -%}
        <span class="{{ item.class | remove_first: '.' }} bg-black p-2">
          {{ item.title }}
        </span>
      {%- else -%}
        <span class="{{ item.class | remove_first: '.' }} p-2">
          {{ item.title }}
        </span>
      {%- endif -%}

    </div>
  </div>
{% endfor %}
