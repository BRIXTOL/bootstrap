  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@brixtol/eslint-config": "workspace:^1.1.0",
    "@brixtol/prettier-config": "workspace:^1.2.0",
    "@brixtol/rollup-config": "workspace:^1.5.0",
    "postcss": "^8.3.6",
    "sass": "^1.41.1",
    "stylelint": "^13.13.1",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-scss": "^3.21.0"
