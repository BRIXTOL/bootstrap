{
  "name": "@brixtol/bootstrap",
  "version": "1.0.0",
  "private": false,
  "description": "Stripped bootstrap grid and utilities",
  "author": {
    "name": "Νίκος Σαβίδης",
    "email": "n@brixtol.com",
    "url": "https://brixtoltextiles.com"
  },
  "keywords": [
    "bootstrap",
    "mcss",
    "css",
    "scss"
  ],
