<div class="row jc-between">

  <div class="col-4 ai-center">
    {% for item in item.classes %}
      <div class="row ai-center">
        <div class="col-4">
          <pre><code>{{ item.class }}</code></pre>
        </div>
        <div class="col-6">
          <span class="{{ item.class | remove_first: '.' }} p-2">
            {{ item.title }}
          </span>
        </div>
      </div>
    {% endfor %}
  </div>
</div>