{
  "prettier.configPath": "node_modules/@brixtol/prettier-config/index.js",
  "stylelint.packageManager": "pnpm",
  "stylelint.enable": false,
  "stylelint.validate": [
    "scss"
  ],
  "scss.validate": false,
  "html-css-class-completion.HTMLLanguages": [
    "html",
    "liquid"
  ],
  "somesass.scannerDepth": 30,
  "somesass.suggestAllFromOpenDocument": true,
  "somesass.suggestFunctionsInStringContextAfterSymbols": " (+-*%'",
  "somesass.suggestionStyle": "all",
  "somesass.scannerExclude": [
    "**/.git/**",
    "**/node_modules/**",
    "**/bower_components/**",
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.stylelint": "explicit"
  },
  "[liquid]": {
    "editor.defaultFormatter": "sissel.shopify-liquid",
    "editor.formatOnSave": true
  },
  "html.customData": [
    "./.vscode/custom-data/accordion.html-data.json",
    "./.vscode/custom-data/carousel.html-data.json",
    "./.vscode/custom-data/dropdown.html-data.json",
    "./.vscode/custom-data/form.html-data.json",
    "./.vscode/custom-data/spx.html-data.json",
    "./.vscode/custom-data/purchase.html-data.json",
    "./.vscode/custom-data/sticky.html-data.json",
    "./.vscode/custom-data/stimulus.html-data.json",
    "./.vscode/custom-data/viewer.html-data.json"
  ],
}